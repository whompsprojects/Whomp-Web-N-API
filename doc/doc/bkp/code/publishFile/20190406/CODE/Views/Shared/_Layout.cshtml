<!DOCTYPE html>
<html>
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <meta http-equiv="X-UA-Compatible" content="IE=10; IE=9; IE=8; IE=7; IE=EDGE" />
    <title>@ViewBag.Title - Whomps </title>
    @Styles.Render("~/Content/css")
    @Scripts.Render("~/bundles/modernizr")
    <link href="~/Content/CssLibrary/bootstrap.css" rel="stylesheet" />
    <link href="~/Content/CssLibrary/fonts.googleapis.com.css" rel="stylesheet" />
    <link href="~/Content/CssLibrary/maxcdn.bootstrapcdn.com.css" rel="stylesheet" />
    <link href="~/Content/CssLibrary/cdnjs.cloudflare.com.css" rel="stylesheet" />
    <link href="~/Content/CssLibrary/formden.com.css" rel="stylesheet" />
    <link href="~/Content/CssLibrary/fontawesome-all.css" rel="stylesheet" />
    <link href="~/Content/CssLibrary/style.css" rel="stylesheet" />
    <link href="~/Content/CssLibrary/owl.carousel.min.css" rel="stylesheet" />
    <link href="~/Content/CssLibrary/jquery-ui-custom.css" rel="stylesheet" />
    <link href="~/Content/MenuCSS/sm-core-css.css" rel="stylesheet" />
    <link href="~/Content/MenuCSS/sm-blue.css" rel="stylesheet" />
    <link href="~/Content/CssLibrary/main.css" rel="stylesheet" />
    <link href="~/Content/CssLibrary/responsive.css" rel="stylesheet" />
    <link href="~/Content/CssLibrary/bootstrap-tagsinput.css" rel="stylesheet" />
    <link href="~/Content/CssLibrary/ui.jqgrid.css" rel="stylesheet" />
    <link href="~/Content/CssLibrary/custom-jqgrid.css" rel="stylesheet" />
    <link href="~/Content/CssLibrary/dataTables_plugin.bootstrap.css" rel="stylesheet" type="text/css" />
    <link href="~/Content/CssLibrary/dataTables.bootstrap.css" rel="stylesheet" type="text/css" />
    <link href="~/Content/CssLibrary/CommonCSS.css" rel="stylesheet" type="text/css" />

    <script src="~/Scripts/Library/jquery-1.10.2.min.js"></script>
    <script src="~/Scripts/Library/jquery-ui.js"></script>
    <script src="~/Scripts/Library/bootstrap.js"></script>
    <script src="~/Scripts/Library/typehead.js"></script>
    <script>
        var layout_MenuDetail = @Html.Raw(Json.Encode(Session["MenuDetail"]));
        var layout_GetMenu_URL = "@Url.Action("GetMenu", "Dashboard")";
        var  First_Name="@Session["First_Name"]";
        var registerUrl = "@Url.Action("Login", "Account")";
        var  UserName="@Session["First_Name"]";
        var AppPath = "@Request.ApplicationPath";
    </script>

</head>
<body>
    <header class="navbar navbar-inverse navbar-main-section navbar-fixed-top">

        <div class="navbar-header">
            <div class="logo-div">
                <button type="button" class="navbar-toggle mobile-btn" data-toggle="collapse" data-target="#nav_layout_Menu">
                    <span class="icon-bar"></span>
                    <span class="icon-bar"></span>
                    <span class="icon-bar"></span>
                </button>
                <img src="~/Content/Images/CompressLogoWhomps.png" class="logo_img" />
            </div>

            <div class="menu">
                <nav id="nav_layout_Menu" class="collapse navbar-collapse"></nav>
            </div>

        </div>
    </header>
    <div class="body-content">
        <div class="breadcrumbb" align="right">
        </div>
        <div class="clearfix container-fluid">
            <div id="err-message-holder" class="alert alert-danger small err-message-holder" style="display:none">
                <i class="fa fa-warning fa-lg"></i>&nbsp;&nbsp;
            </div>
        </div>
        <p>
            @RenderBody()
        </p>
    </div>
    <footer class="footer-main-box">
        <div class="row">
            <div class="col-xs-12 col-md-4 footer-logo">
                @*<img src="~/Content/Images/prestige_Logo.png" />*@
                <img src="~/Content/Images/CompressLogoWhomps.png" />
            </div>
            <div class="col-xs-12 col-md-6 copyRight-div">
                <p>
                    Whomps
                    <br />Copyright &copy; @DateTime.Now.Year -  Whomps. All Rights Reserved.
                </p>
            </div>
        </div>
    </footer>
    @*Alert PopUp*@
    <div id="div_Error" class="modal fade" role="dialog">
    </div>
    @*<%--========================================Start of Processing==================================--%>*@
    <div id="processing-bar-holder" class="clearfix processing-bar-holder in">
        <div class="processing-bar">
            <i class="fa fa-spinner fa-spin"></i><span>&nbsp;&nbsp;Processing</span>
        </div>
    </div>
    <div class="modaloverlay" id="modaloverlay" style="display: block;">
    </div>
    @*<%--========================================End of Processing==================================--%>*@
    @Scripts.Render("~/bundles/bootstrap")
    @RenderSection("scripts", required: false)
    <script src="~/Scripts/Library/cdnjs.cloudflare.com.js"></script>
    <script src="~/Scripts/Library/jquery.smartmenus.js"></script>
    <script src="~/Scripts/Library/grid.locale-en.js"></script>
    <script src="~/Scripts/Library/jquery.jqGrid.min.js"></script>
    <script src="~/Scripts/CommonJS/CommonFn.js"></script>
    <script src="~/Scripts/CommonJS/Convert.js"></script>
    <script src="~/Scripts/CommonJS/_Layout.js"></script>
    <script src="~/Scripts/Library/jquery.dataTables.min.js" type="text/javascript"></script>
    <script src="~/Scripts/Library/dataTables.bootstrap.js" type="text/javascript"></script>
    @*<%--======================Download excel from datatable======================--%>*@
    <script type="text/javascript" src="~/Scripts/Library/dataTables.buttons.min.js"></script>
    <script type="text/javascript" src="~/Scripts/Library/buttons.flash.min.js"></script>
    <script type="text/javascript" src="~/Scripts/Library/jszip.min.js"></script>
    <script type="text/javascript" src="~/Scripts/Library/buttons.html5.min.js"></script>
    <!-- SmartMenus jQuery init -->
    <script type="text/javascript">
        $(function () {
            $('#main-menu').smartmenus({
                subMenusSubOffsetX: 1,
                subMenusSubOffsetY: -8
            });
        });
    </script>
</body>
</html>
